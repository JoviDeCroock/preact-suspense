{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2021.WeakRef", "ES2015.Symbol", "DOM", "ESNext.Disposable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "jsxFragmentFactory": "Fragment",
    "stripInternal": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["src", "./vitest.config.ts"]
}